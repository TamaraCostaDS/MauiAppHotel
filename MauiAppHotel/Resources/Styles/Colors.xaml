<?xml version="1.0" encoding="UTF-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <Color x:Key="Primary">#2B8B9B</Color>
    <Color x:Key="Secondary">#F28482</Color>
    <Color x:Key="PageBackground">#F7F2E8</Color>
    <Color x:Key="DarkText">#262626</Color>
    <Color x:Key="LightText">#FAFAFA</Color>
    <Color x:Key="FrameBackground">#FFFFFF</Color>
    <Color x:Key="Gray900">#2B8B9B</Color>
    <Color x:Key="Gray700">#666666</Color>
    <Color x:Key="Gray300">#BBBBBB</Color>
    <Color x:Key="Gray100">#F7F2E8</Color>
    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource Primary}" />
    <SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource Secondary}" />
    <SolidColorBrush x:Key="DarkTextBrush" Color="{StaticResource DarkText}" />
    <SolidColorBrush x:Key="LightTextBrush" Color="{StaticResource LightText}" />
</ResourceDictionary>